<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane xmlns="http://javafx.com/javafx"
                xmlns:fx="http://javafx.com/fxml"
                fx:id="settingPage"
                styleClass="root-pane"
                fx:controller="com.gnome.gnome.setting.SettingController"
                prefHeight="400.0" prefWidth="600.0">

    <top>
        <VBox alignment="CENTER" spacing="20" style="-fx-padding: 20">
            <Label text="Setting"  styleClass="menu-title"/>

            <HBox alignment="CENTER" maxWidth="500" spacing="10">
                <Button fx:id="backGameButton" styleClass="menu-button" text="back to game" onAction="#backToGameButtonClick" prefWidth="230"/>
                <Button fx:id="meinMenuButton" styleClass="menu-button" text="mein menu" onAction="#meinMenuButtonClick" prefWidth="230"/>
            </HBox>
        </VBox>
    </top>
    <center>
        <VBox alignment="CENTER" spacing="10">
            <Label text="Here is Setting:" styleClass="menu-title"/>
            <Label text="1. Setting" styleClass="center-button"/>
            <Label text="2. Setting" styleClass="center-button"/>
        </VBox>
    </center>



</BorderPane>
