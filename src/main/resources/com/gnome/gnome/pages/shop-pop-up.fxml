<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:id="shopPopUpPage"
            styleClass="root-pane"
            fx:controller="com.gnome.gnome.shopPopUp.ShopPopUpController"
            prefHeight="400.0" prefWidth="600.0">

<top>
    <VBox alignment="CENTER" spacing="20" style="-fx-padding: 20">
        <Label text="New Game"  styleClass="menu-title"/>

        <HBox alignment="CENTER" maxWidth="400" spacing="10">
            <Button fx:id="ReturnGameButton" styleClass="menu-button" text="Return Game" onAction="#newGameButtonClick" />
        </HBox>
    </VBox>
</top>
<center>
    <Label text="Here is Shop POPUP"  styleClass="center-title"/>
</center>



</BorderPane>
